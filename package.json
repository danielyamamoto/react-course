{
    "name": "react-course",
    "version": "0.1.0",
    "private": true,
    "description": "Landing page for react-course",
    "dependencies": {
        "radium": "^0.26.1",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-scripts": "^4.0.2"
    },
    "scripts": {
        "watch:sass": "node-sass public/sass/main.scss public/css/style.css -w",

        "compile:sass": "node-sass public/sass/main.scss public/css/style.comp.css",
        "concat:css": "concat -o public/css/style.concat.css public/css/normalize.css public/css/style.comp.css",
        "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' public/css/style.concat.css -o public/css/style.prefix.css",
        "compress:css": "node-sass public/css/style.prefix.css public/css/style.css --output-style compressed",
        "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css",

        "start-js": "react-scripts start",
        "start": "npm-run-all --parallel watch:sass start-js",
        "build": "react-scripts build",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "author": "Daniel Yamamoto",
    "license": "ISC",
    "devDependencies": {
        "autoprefixer": "^10.2.4",
        "concat": "1.0.3",
        "node-sass": "^5.0.0",
        "npm-run-all": "^4.1.5",
        "postcss-cli": "^8.3.1"
    }
}
